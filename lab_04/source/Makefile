CC := gcc
CFLAGS := -g3 -Wall -Werror
SROUCE_DIR := .
BUILDDIR := ../bin
TARGET := app.exe
SOURCES := $(wildcard $(SROUCE_DIR)/*.c)
OBJECTS := $(patsubst $(SROUCE_DIR)/%.o,$(BUILDDIR)/%.o,$(SOURCES:.c=.o))

$(BUILDDIR)/$(TARGET): $(OBJECTS)
	$(CC) -o $@ $^

$(BUILDDIR)/%.o: $(SROUCE_DIR)/%.c
	$(CC) $< $(CFLAGS) -c -o $@

clean:
	-rm $(BUILDDIR)/$(TARGET) $(OBJECTS)
